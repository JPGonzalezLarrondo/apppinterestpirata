<p id="notice"><%= notice %></p>
<div class="container">
	<p>
	  <strong>FOTO: </strong>
	  <%= image_tag(@pin.photo.url, :style=> 'width:200px') %>
	</p>

	<p>
	  <strong>Description:</strong>
